    "url": "git+https://github.com/lbonnage/list-bot.git"
  },
  "author": "Liam Bonnage",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lbonnage/list-bot/issues"
  },
  "homepage": "https://github.com/lbonnage/list-bot#readme",
  "dependencies": {
    "@types/node": "^15.14.0",
    "@types/validator": "^13.6.0",
    "discord-api-types": "^0.18.1",
    "discord.js": "^13.0.0-dev.0d0c8f07f2535dd315ca95abe35e848c13d0ddf5",
    "dotenv": "^10.0.0",
    "sequelize": "^6.6.4",
    "sqlite3": "^5.0.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.1",
    "eslint": "^7.29.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "typescript": "^4.3.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint . --fix",
      "git add"
    ]
  },
  "type": "module"
}
